{
  "name": "synth-control",
  "description": "Controls for a Javascript electronic or web synth",
  "version": "0.0.0",
  "author": {
    "name": "David Ryan",
    "email": "davidryan1998@hotmail.com",
    "url": "https://github.com/davidryan59"
  },
  "repository": "git@github.com:davidryan59/synth-control.git",
  "license": "ISC",
  "main": "dist/src/index.js",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "lint": "eslint lib",
    "lint:fix": "eslint lib --fix",
    "test": "jest --test-path-pattern=lib/test --watch-all=true --coverage=true",
    "test:ppo": "jest --test-path-pattern=lib/test",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "examples": "cd ./dist/examples && node run.js",
    "prepublishOnly": "npm run lint && npm run test:ppo && npm run build"
  },
  "dependencies": {
    "is-object": "^1.0.1",
    "isarray": "^2.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1"
  },
  "keywords": [
    "api",
    "audio",
    "control",
    "js",
    "synth",
    "tone",
    "tone-js",
    "web"
  ]
}
